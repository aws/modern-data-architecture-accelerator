{
  "name": "@aws-mdaa/mdaa",
  "private": true,
  "bin": "./packages/cli/bin/mdaa",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "git-clean": "git clean -Xfd -e !development -e !development/** -e !.vscode -e !.vscode/** -e !.idea -e !.idea/**",
    "lint": "lerna run lint --stream",
    "prepare": "husky install source/.husky",
    "prettier": "lerna run prettier --stream",
    "test:snapshots": "jest --config jest.config.snapshot.js",
    "test:snapshots:update": "jest --config jest.config.snapshot.js --updateSnapshot",
    "test:python": "lerna run test:python --stream",
    "test:python:install": "lerna run test:python:install --stream",
    "test:python:all": "./scripts/python_test.sh",
    "delete:snapshots": "find . -path '*/__snapshots__/*.snap' -type f -delete"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --max-warnings 0 -c .eslintrc.json --ignore-pattern \"*.d.ts\""
  },
  "devDependencies": {
    "@types/jest": "29.5.0",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-prettier": "4.2.1",
    "lint-staged": "15.2.10",
    "husky-init": "8.0.0",
    "prettier": "2.8.4",
    "jsii": "5.7.5",
    "jsii-config": "1.107.0",
    "jsii-pacmak": "1.107.0",
    "typedoc": "0.24.8"
  },
  "optionalDependencies": {
    "@nx/nx-darwin-arm64": "20.5.0",
    "@nx/nx-darwin-x64": "20.5.0",
    "@nx/nx-freebsd-x64": "20.5.0",
    "@nx/nx-linux-arm-gnueabihf": "20.5.0",
    "@nx/nx-linux-arm64-gnu": "20.5.0",
    "@nx/nx-linux-arm64-musl": "20.5.0",
    "@nx/nx-linux-x64-gnu": "20.5.0",
    "@nx/nx-linux-x64-musl": "20.5.0",
    "@nx/nx-win32-arm64-msvc": "20.5.0",
    "@nx/nx-win32-x64-msvc": "20.5.0"
  },
  "dependencies": {
    "typescript": "4.6.3",
    "lerna": "8.2.1",
    "nx": "20.5.0",
    "aws-cdk": "2.1019.0"
  },
  "overrides": {
    "semver": "7.5.2",
    "axios": "1.8.2",
    "aws-cdk-lib": "2.201.0",
    "@aws-cdk/aws-redshift-alpha": "2.201.0-alpha.0",
    "@aws-cdk/aws-glue-alpha": "2.201.0-alpha.0",
    "cross-spawn": "7.0.5",
    "@octokit/request": "9.2.1",
    "cdk-nag": "2.37.1",
    "cdk8s": "2.69.42",
    "tar": "6.2.1",
    "glob": "9.3.5",
    "follow-redirects": "1.15.6",
    "@types/babel__traverse": "7.20.6",
    "cdk8s@>=2.68.32": {
      "backport": "9.4.3",
      "follow-redirects": "1.15.6"
    }
  },
  "workspaces": [
    "packages/cli",
    "packages/constructs/L2/*",
    "packages/constructs/L3/*/*",
    "packages/utilities/*",
    "packages/apps/*/*",
    "schemas"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/modern-data-architecture-accelerator"
  }
}
