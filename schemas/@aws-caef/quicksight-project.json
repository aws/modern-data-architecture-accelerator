                    "maxItems": 1,
                    "minItems": 1,
                    "type": "array"
                },
                "password": {
                    "description": "Password",
                    "type": "string"
                },
                "username": {
                    "description": "Username",
                    "type": "string"
                }
            },
            "required": [
                "password",
                "username"
            ],
            "type": "object"
        },
