{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "test"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "{projectRoot}/package.json",
        "{projectRoot}/bin/*.ts",
        "{projectRoot}/lib/*.ts",
        "{projectRoot}/test/*.ts"
      ],
      "outputs": [
        "{projectRoot}/bin/*.d.ts",
        "{projectRoot}/bin/*.js",
        "{projectRoot}/lib/*.d.ts",
        "{projectRoot}/lib/*.js",
        "{projectRoot}/test/*.d.ts",
        "{projectRoot}/test/*.js"
      ]
    },
    "test": {
      "inputs": [
        "{projectRoot}/bin/*.ts",
        "{projectRoot}/lib/*.ts",
        "{projectRoot}/test/*.ts",
        "{projectRoot}/test/*.yaml"
      ],
      "outputs": [
        "{projectRoot}/coverage"
      ]
    }
  }
}