                            "id": {
                                "type": "string"
                            },
                            "reason": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "id",
                            "reason"
                        ],
                        "type": "object"
                    },
                    "type": "array"
                }
            },
            "required": [
                "path",
                "suppressions"
            ],
            "type": "object"
        },
        "CaefNagSuppressions": {
            "additionalProperties": false,
            "properties": {
                "by_path": {
                    "items": {
                        "$ref": "#/definitions/CaefNagSuppressionByPath"
                    },
                    "type": "array"
                }
            },
            "required": [
                "by_path"
            ],
            "type": "object"
        },
        "CaefServiceCatalogConstraintConfig": {
            "additionalProperties": false,
            "properties": {
                "description": {
                    "type": "string"
                },
                "rules": {
                    "additionalProperties": {
                        "$ref": "#/definitions/CaefServiceCatalogConstraintRuleConfig"
                    },
                    "type": "object"
                }
            },
            "required": [
                "description",
                "rules"
            ],
            "type": "object"
        },
        "CaefServiceCatalogConstraintRuleAssertionConfig": {
            "additionalProperties": false,
            "properties": {
                "assert": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                }
            },
            "required": [
                "assert",
                "description"
            ],
            "type": "object"
        },
        "CaefServiceCatalogConstraintRuleCondititionConfig": {
            "additionalProperties": false,
            "type": "object"
        },
        "CaefServiceCatalogConstraintRuleConfig": {
            "additionalProperties": false,
            "properties": {
                "assertions": {
                    "items": {
                        "$ref": "#/definitions/CaefServiceCatalogConstraintRuleAssertionConfig"
                    },
                    "type": "array"
                },
                "condition": {
                    "$ref": "#/definitions/CaefServiceCatalogConstraintRuleCondititionConfig"
                }
            },
            "required": [
                "assertions",
                "condition"
            ],
            "type": "object"
        },
        "CaefServiceCatalogParameterConfig": {
            "additionalProperties": false,
            "properties": {
                "constraints": {
                    "$ref": "#/definitions/CaefServiceCatalogConstraintConfig"
                },
                "props": {
                    "$ref": "#/definitions/CfnParameterProps"
                }
            },
            "required": [
                "props"
            ],
            "type": "object"
        },
        "CaefServiceCatalogProductConfig": {
            "additionalProperties": false,
            "properties": {
                "launch_role_name": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "owner": {
                    "type": "string"
                },
                "parameters": {
                    "additionalProperties": {
                        "$ref": "#/definitions/CaefServiceCatalogParameterConfig"
                    },
                    "type": "object"
                },
                "portfolio_arn": {
                    "type": "string"
                }
            },
            "required": [
                "name",
                "owner",
                "portfolio_arn"
            ],
            "type": "object"
        },
        "CfnParameterProps": {
            "additionalProperties": false,
            "properties": {
                "allowedPattern": {
                    "default": "- No constraints on patterns allowed for parameter.",
                    "description": "A regular expression that represents the patterns to allow for String types.",
                    "type": "string"
                },
                "allowedValues": {
                    "default": "- No constraints on values allowed for parameter.",
                    "description": "An array containing the list of values allowed for the parameter.",
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "constraintDescription": {
                    "default": "- No description with customized error message when user specifies invalid values.",
                    "description": "A string that explains a constraint when the constraint is violated.\nFor example, without a constraint description, a parameter that has an allowed\npattern of [A-Za-z0-9]+ displays the following error message when the user specifies\nan invalid value:",
                    "type": "string"
                },
                "default": {
                    "default": "- No default value for parameter.",
                    "description": "A value of the appropriate type for the template to use if no value is specified\nwhen a stack is created. If you define constraints for the parameter, you must specify\na value that adheres to those constraints."
                },
                "description": {
                    "default": "- No description for the parameter.",
                    "description": "A string of up to 4000 characters that describes the parameter.",
                    "type": "string"
                },
                "maxLength": {
                    "default": "- None.",
                    "description": "An integer value that determines the largest number of characters you want to allow for String types.",
                    "type": "number"
                },
                "maxValue": {
                    "default": "- None.",
                    "description": "A numeric value that determines the largest numeric value you want to allow for Number types.",
                    "type": "number"
                },
                "minLength": {
                    "default": "- None.",
                    "description": "An integer value that determines the smallest number of characters you want to allow for String types.",
                    "type": "number"
                },
                "minValue": {
                    "default": "- None.",
                    "description": "A numeric value that determines the smallest numeric value you want to allow for Number types.",
                    "type": "number"
                },
                "noEcho": {
                    "default": "- Parameter values are not masked.",
                    "description": "Whether to mask the parameter value when anyone makes a call that describes the stack.\nIf you set the value to ``true``, the parameter value is masked with asterisks (``*****``).",
                    "type": "boolean"
                },
                "type": {
                    "default": "String",
                    "description": "The data type for the parameter (DataType).",
                    "type": "string"
                }
            },
            "type": "object"
        },
        "FederationProps": {
            "additionalProperties": false,
            "properties": {
                "providerArn": {
                    "description": "Arn of an existing provider",
                    "type": "string"
                },
                "samlDoc": {
                    "description": "Path to a SAML doc to be used to create a new provider",
                    "type": "string"
                }
            },
            "type": "object"
        },
        "GenerateManagedPolicyConfig": {
            "additionalProperties": false,
            "properties": {
                "policyDocument": {
                    "additionalProperties": {},
                    "type": "object"
                },
                "suppressions": {
                    "items": {
                        "$ref": "#/definitions/SuppressionProps"
                    },
                    "type": "array"
                },
                "verbatimPolicyName": {
                    "type": "boolean"
                }
            },
            "required": [
                "policyDocument"
            ],
            "type": "object"
        },
        "GenerateRoleProps": {
            "additionalProperties": false,
            "properties": {
                "additionalTrustedPrincipals": {
                    "description": "Additional assume role trust configs.",
                    "items": {
                        "$ref": "#/definitions/TrustedPrincipalProps"
                    },
                    "type": "array"
                },
                "assumeRoleTrustConditions": {
                    "additionalProperties": {},
                    "description": "Conditions to apply to the assume role trust policy",
                    "type": "object"
                },
                "awsManagedPolicies": {
                    "description": "List of AWS Managed policies to associate to the role.",
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "basePersona": {
                    "description": "Intended base persona that the generated role should mimic\nAll the policies associated with specified persona will get associated with the generated role",
                    "enum": [
                        "data-admin",
                        "data-engineer",
                        "data-scientist"
                    ],
                    "type": "string"
                },
                "customerManagedPolicies": {
                    "description": "List of AWS Managed policies to associate to the role.",
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "generatedPolicies": {
                    "description": "List of generated policies to associate to the role.",
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "suppressions": {
                    "description": "Suppressions if required by the role configuration.",
                    "items": {
                        "$ref": "#/definitions/SuppressionProps"
                    },
                    "type": "array"
                },
                "trustedPrincipal": {
                    "description": "The assume role trust config.",
                    "type": "string"
                },
                "usageProfile": {
                    "description": "Usage Profile to be used to define the role",
                    "enum": [
                        "data-admin",
                        "data-engineer",
                        "data-scientist",
                        "data-steward"
                    ],
                    "type": "string"
                }
            },
            "required": [
                "trustedPrincipal"
            ],
            "type": "object"
        },
        "SuppressionProps": {
            "additionalProperties": false,
            "properties": {
                "id": {
                    "type": "string"
                },
                "reason": {
                    "type": "string"
                }
            },
            "required": [
                "id",
                "reason"
            ],
            "type": "object"
        },
        "TrustedPrincipalProps": {
            "additionalProperties": false,
            "properties": {
                "additionalTrustedActions": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "trustedPrincipal": {
                    "description": "The assume role trust config.",
                    "type": "string"
                }
            },
            "required": [
                "trustedPrincipal"
            ],
            "type": "object"
        }
    },
    "properties": {
        "federations": {
            "additionalProperties": {
                "$ref": "#/definitions/FederationProps"
            },
            "description": "List of federations which may be referenced by generated roles.",
            "type": "object"
        },
        "generatePolicies": {
            "additionalProperties": {
                "$ref": "#/definitions/GenerateManagedPolicyConfig"
            },
            "description": "List of managed policies to generate.",
            "type": "object"
        },
        "generateRoles": {
            "additionalProperties": {
                "$ref": "#/definitions/GenerateRoleProps"
            },
            "description": "List of roles to generate.",
            "type": "object"
        },
        "nag_suppressions": {
            "$ref": "#/definitions/CaefNagSuppressions",
            "description": "Nag suppressions"
        },
        "service_catalog_product_config": {
            "$ref": "#/definitions/CaefServiceCatalogProductConfig",
            "description": "Service Catalog Config\nIf specified, the configured module will be deployed as a Service Catalog product instead of directly to the environment"
        }
    },
    "type": "object"
}

