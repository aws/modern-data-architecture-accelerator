{
  "name": "@aws-mdaa/installer",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "bin": {
    "mdaa-installer": "bin/mdaa-installer.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:unit": "jest --testPathIgnorePatterns=snapshot",
    "test:snapshot": "jest --testPathPattern=snapshot",
    "test:snapshot:update": "jest --config jest.config.js --updateSnapshot",
    "cdk": "cdk",
    "lint": "eslint --max-warnings 0 -c ../.eslintrc.json '**/*.{ts,tsx}' --ignore-pattern 'dist/*' --ignore-pattern 'node_modules/*' --ignore-pattern \"*.d.ts\" "
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "aws-cdk": "2.1019.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.201.0",
    "constructs": "^10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/modern-data-architecture-accelerator"
  }
}
